#ifndef _STDCPP_FUNCTIONAL_
#define _STDCPP_FUNCTIONAL_


namespace std {

template< class T >
class less {
public:
	bool operator()( const T& lhs, const T& rhs ) const {
		return lhs < rhs;
	}
};

template< class T >
class greater {
public:
	bool operator()( const T& lhs, const T& rhs ) const {
		return lhs > rhs;
	}
};

template<class Key>
struct hash;

#include "internal/hash.tpp"

}

#endif // _STDCPP_FUNCTIONAL_
