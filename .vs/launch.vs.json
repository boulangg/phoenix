{
  "version": "0.2.1",
  "defaults": {},
  "configurations": [
    {
      "type": "cppgdb",
      "name": "_debug_unit_test",
      "project": "CMakeLists.txt",
      "projectTarget": "unittestsuite",
      "comment": "Learn how to configure WSL debugging. For more info, see http://aka.ms/vslinuxdebug",
      "debuggerConfiguration": "gdb",
      "args": [ "./libunittestsuite.so" ],
      "env": {},
      "cwd ": "cd ~/.vs/phoenix/build/linux-default/test;",
      "program": "./dependency/UnitTestFramework/unittest_runner"
    }
  ]
}
